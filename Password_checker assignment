def password_checker() -> str:
    """
    Prompts the user to enter a password with a maximum of five attempts.
    
    :Return: a message indicating whether login was successful or access was denied.
    :rtype: str 
    """
    correct_password = "python123"
    trials = 5

    while trials > 0:
        password = input("Enter your password: ")

        if password == correct_password:
            return "Login successful"
        else:
            trials -= 1
            if trials > 0:
                print(f"Incorrect password. You have {trials} trial(s) remaining.")

    return "You are out of trials. Access denied."


result = password_checker()
print(result)
